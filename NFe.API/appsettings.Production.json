{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${PROD_DB_HOST};Database=${PROD_DB_DATABASE};Username=${PROD_DB_USERNAME};Password=${PROD_DB_PASSWORD}"
  },
  "AzureKeyVault": {
    "Url": "${AZURE_KEYVAULT_URL}",
    "CertificateName": "${AZURE_CERTIFICATE_NAME}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
